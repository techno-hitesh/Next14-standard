{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "outDir": "./dist",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/store/reduxProvider.tsx",
    "app/store/slices/userSlicer.js",
    "app/store/rootReducer.js",
    "app/(view)/admin/components/piechart.tsx",
    "app/(view)/admin/context/UserContext.js",
    "dist/types/**/*.ts"
, "app/(view)/dashboard/review/id"  ],
  "exclude": [
    "node_modules"
  ]
}
